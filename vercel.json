{
    "version": 2,
    "name": "pokemon",
    "builds": [
        { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/index.js" }
    ],
    "env": {
        "BRANCH": "vercel",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://pokemon-nine-roan.vercel.app/"
    }
}